{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "name": "Accompany-Me",
    "slug": "Accompany-Me",
    "version": "1.2.6",
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "icon": "./assets/icons/icon.png",
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      "expo-apple-authentication",
      "expo-image-picker"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.javascripted.Accompany-Me",
      "useAppleSignIn": true,
      "buildNumber": "26",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app uses your photo library to let you choose images for profile photos and calendar customization. This enhances your app experience by allowing for a personalized planning environment."
      }
    },
    "android": {
      "package": "com.javascripted.Accompany-Me"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d522e45b-646f-4f0f-9586-2c539eb85887"
      }
    }
  }
}
